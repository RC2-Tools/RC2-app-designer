{{#> layout }}
    {{#*inline "mainContent" }}
        {{> pageTitle token="location" }}

        <select id="choose_dept" class="custom-select text-center rounded-pill w-75 mx-auto mb-3"></select>
        <select id="choose_pam" class="custom-select text-center rounded-pill w-75 mx-auto mb-3"></select>

        {{> menuBtn
                id="continue"
                icon="../img/enter_icon.svg"
                label="continue"
        }}
    {{/inline}}

    {{#*inline "bodyEnd" }}
        <script type="text/javascript" src="../libs/underscore-1.9.1.min.js"></script>
        <script type="text/javascript" src="../libs/jquery.csv-1.0.5.min.js"></script>
        <script type="text/javascript" src="../js/renderers/choose_location.js"></script>

        <script>
            'use strict';
            $(util.renderPage(display));
        </script>
    {{/inline}}
{{/layout}}
