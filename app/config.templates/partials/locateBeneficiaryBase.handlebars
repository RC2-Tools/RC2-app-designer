{{#> layout }}
    {{#*inline "mainContent" }}
        {{> pageTitle token=title }}

        {{#> pageSpecific }}{{/pageSpecific}}

        {{> menuBtn
                id="barcode"
                icon="../img/barcode_icon.svg"
                label="scan_barcode"
        }}

        {{> textInput id="code" }}

        {{> menuBtn
                id="enter"
                icon="../img/enter_icon.svg"
                label="enter"
        }}

        {{> textBlock id="search_results" }}
    {{/inline}}

    {{#*inline "bodyEnd" }}
        <script type="text/javascript" src="../libs/underscore-1.9.1.min.js"></script>
        <script type="text/javascript" src="../js/chooseBeneficiaryUtil.js"></script>
        {{> pageScripts }}

        <script>
            'use strict';
            $(util.renderPage(display));
        </script>
    {{/inline}}
{{/layout}}
