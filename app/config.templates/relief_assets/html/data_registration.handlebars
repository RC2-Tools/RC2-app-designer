{{#> reliefLayout }}
    {{#*inline "mainContent" }}
        {{> pageTitle token="registration_data" }}

        {{> menuItem
                id="enabledBeneficiaryEnt"
                icon=(concat @root.assetsPath "/img/reg_icon.svg")
        }}

        {{> menuItem
                id="disabledBeneficiaryEnt"
                icon=(concat @root.assetsPath "/img/reg_icon.svg")
        }}

        {{> menuItem
                id="searchBeneficiaryEnt"
                icon=(concat @root.assetsPath "/img/search_icon.svg")
        }}

        {{> menuItem
                id="searchMember"
                icon=(concat @root.assetsPath "/img/search_icon.svg")
                label="search_members"
        }}
    {{/inline}}

    {{#*inline "bodyEnd" }}
        <script type="text/javascript" src="{{ @root.reliefAssetsPath }}/js/renderers/data_registration.js"></script>

        <script>
            'use strict';
            $(util.renderPage(display));
        </script>
    {{/inline}}
{{/reliefLayout}}
