{{#> reliefLayout }}
    {{#*inline "mainContent" }}
        {{> pageTitle token="data_categories" }}

        {{> menuItem
                id="beneficiary"
                icon=(concat @root.assetsPath "/img/reg_icon.svg")
                label="view_registration_data"
        }}

        {{> menuItem
                id="delivery"
                icon=(concat @root.assetsPath "/img/dist_icon.svg")
                label="view_delivery_data"
        }}
    {{/inline}}

    {{#*inline "bodyEnd" }}
        <script type="text/javascript" src="{{ @root.reliefAssetsPath }}/js/renderers/data_start.js"></script>

        <script>
            'use strict';
            $(util.renderPage(display));
        </script>
    {{/inline}}
{{/reliefLayout}}
