{{#> healthLayout }}
    {{#*inline "mainContent" }}
        {{> pageTitle token=title }}

        {{#> pageSpecific }}{{/pageSpecific}}

        {{> menuBtn
                id="barcode"
                icon=(concat @root.assetsPath "/img/barcode_icon.svg")
                label="scan_barcode"
        }}

        {{> textInput id="code" }}

        {{> menuBtn
                id="enter"
                icon=(concat @root.assetsPath "/img/enter_icon.svg")
                label="enter"
        }}

        {{> textBlock id="search_results" }}
    {{/inline}}

    {{#*inline "bodyEnd" }}
        <script type="text/javascript" src="{{ @root.assetsPath }}/libs/underscore-1.9.1.min.js"></script>
        <script type="text/javascript" src="{{ @root.assetsPath }}/js/chooseBeneficiaryUtil.js"></script>
        {{> pageScripts }}

        <script>
            'use strict';
            $(util.renderPage(display));
        </script>
    {{/inline}}
{{/healthLayout}}
