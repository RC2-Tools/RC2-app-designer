{{#> reliefLayout }}
    {{#*inline "mainContent" }}
        {{> pageTitle token="administrator_options" }}

        {{> menuItem
                id="registration"
                icon=(concat @root.assetsPath "/img/reg_icon.svg")
                label="registration"
        }}

        {{> menuItem
                id="distribution"
                icon=(concat @root.assetsPath "/img/dist_icon.svg")
                label="distribution"
        }}
    {{/inline}}

    {{#*inline "bodyEnd" }}
        <script type="text/javascript" src="{{ @root.reliefAssetsPath }}/js/renderers/overrides_start.js"></script>

        <script>
            'use strict';
            $(util.renderPage(display));
        </script>
    {{/inline}}
{{/reliefLayout}}
