{{#> reliefLayout }}
    {{#*inline "mainContent" }}
        {{> pageTitle token="delivery_data" }}

        {{> menuItem
                id="viewAllDelivery"
                class="workflow-none"
                icon=(concat @root.assetsPath "/img/dist_icon.svg")
                label="view_all_deliveries"
        }}

        {{> menuItem
                id="viewByAuthorization"
                class="workflow-required workflow-optional workflow-id-only"
                icon=(concat @root.assetsPath "/img/dist_icon.svg")
                label="view_by_authorization"
        }}

        {{> menuItem
                id="deliverySearch"
                class="workflow-required workflow-optional workflow-id-only workflow-none"
                icon=(concat @root.assetsPath "/img/search_icon.svg")
                label="advanced_search"
        }}
    {{/inline}}

    {{#*inline "bodyEnd" }}
        <script type="text/javascript" src="{{ @root.reliefAssetsPath }}/js/renderers/data_delivery.js"></script>

        <script>
            'use strict';
            $(util.renderPage(display));
        </script>
    {{/inline}}
{{/reliefLayout}}
