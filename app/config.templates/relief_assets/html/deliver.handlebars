{{#> reliefLayout }}
    {{#*inline "mainContent" }}
        {{> pageTitle token="deliver_items" }}

        {{> menuBtn
                id="deliver"
                icon=(concat @root.assetsPath "/img/enter_icon.svg")
                label="confirm_delivery"
        }}

        {{> textBlock id="confirmation" }}
    {{/inline}}

    {{#*inline "bodyEnd" }}
        <script type="text/javascript" src="{{ @root.reliefAssetsPath }}/js/renderers/deliver.js"></script>

        <script>
            'use strict';
            $(util.renderPage(display));
        </script>
    {{/inline}}
{{/reliefLayout}}
